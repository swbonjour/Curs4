{
  "name": "quasar-project",
  "version": "0.0.1",
  "description": "A Quasar Project",
  "productName": "Quasar App",
  "author": "",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "swaggen": "swagger-typescript-api -d -p http://127.0.0.1:3000/api-json -o ./src/api/ -n client.ts --api-class-name LangApi --unwrap-response-data --extract-request-params --extract-enums --axios"
  },
  "dependencies": {
    "@babel/core": "^7.23.9",
    "@quasar/extras": "^1.16.4",
    "axios": "^1.6.2",
    "pinia": "^2.1.7",
    "quasar": "^2.6.0",
    "swagger-typescript-api": "^13.0.3",
    "vue": "^3.0.0",
    "vue-axios": "^3.5.2",
    "vue-jwt-decode": "^0.1.0",
    "vue-router": "^4.0.0",
    "vuejs-jwt": "^1.1.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.3.0",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
